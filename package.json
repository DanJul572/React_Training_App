{
    "name": "zapp",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "run:dev": "vite --mode development",
        "run:stage": "vite --mode staging",
        "run:prod": "vite --mode production",
        "build:dev": "tsc -b && vite build --mode development",
        "build:stage": "tsc -b && vite build --mode staging",
        "build:prod": "tsc -b && vite build --mode production",
        "preview:dev": "vite preview --mode development",
        "preview:stage": "vite preview --mode staging",
        "preview:prod": "vite preview --mode production",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "prepare": "husky"
    },
    "dependencies": {
        "@emotion/react": "^11.12.0",
        "@emotion/styled": "^11.12.0",
        "@fontsource/roboto": "^5.0.13",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4",
        "@mui/x-data-grid": "^7.11.0",
        "@types/react-dom": "^18.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.3.2",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^18.3.3",
        "@typescript-eslint/eslint-plugin": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "@vitest/coverage-v8": "^2.0.3",
        "@vitest/ui": "^2.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.8",
        "husky": "^9.1.1",
        "jsdom": "^24.1.0",
        "typescript": "^5.5.3",
        "vitest": "^2.0.3"
    },
    "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
